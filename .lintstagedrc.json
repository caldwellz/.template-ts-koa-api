{
  "*.?(c|m)js?(x)": [
    "eslint --no-ignore --fix",
    "prettier --write --ignore-unknown"
  ],
  "*.json": [
    "eslint --no-ignore --no-eslintrc -c .eslintrc-json.json --fix",
    "prettier --write --ignore-unknown"
  ],
  "*.ts?(x)": [
    "tsc --noEmit --strict --esModuleInterop",
    "eslint --no-ignore --fix",
    "prettier --write --ignore-unknown"
  ]
}
